{
  "compilerOptions": {
    "target": "es2017",
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./src_parser"
  },
  "include": ["src_parser/**/*"],
  "exclude": ["node_modules"]
} 